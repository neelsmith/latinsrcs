    <rdfNamespace abbr="hclat" fullValue="http://shot.holycross.edu/rdf/hclat/"/>
    
 <rdfNamespace abbr="rdfs" fullValue="http://www.w3.org/2000/01/rdf-schema#"/>
    
    
    
    
    <index verb="hclat:detailsFor" inverse="hclat:hasDetails">
        <source type="file" value="verbDetails.csv"></source>
