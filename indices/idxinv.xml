    <index verb="cite:illustrates" inverse="cite:illustratedBy">
        <source type="file" value="imgToEdition.csv"></source>
    </index> 


    <index verb="cite:illustrates" inverse="cite:illustratedBy">
        <source type="file" value="wholeImgToText.csv"></source>
    </index> 


    <index verb="cite:illustrates" inverse="cite:illustratedBy">
        <source type="file" value="imgToSite.csv"></source>
    </index>

    <index verb="cite:illustrates" inverse="cite:illustratedBy">
        <source type="file" value="imgToObject.csv"></source>
    </index>
    
    
    <index verb="hmt:isDefaultImage" inverse="hmt:hasDefaultImage">
    <source type="file" value="preferredImg.csv"/>
    </index>


</indexInventory>
